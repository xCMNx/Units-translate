<Application x:Class="Units_translate.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Units_translate"
             xmlns:UI="clr-namespace:Ui.Converters;assembly=Ui"
             StartupUri="MainWindow.xaml" Exit="Application_Exit">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\ScrollBarTemplate.xaml"/>
                <ResourceDictionary Source=".\Icons.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <UI:ObjectToVisibility x:Key="ObjToVisCnv"/>
            <UI:ObjectToVisibility x:Key="ObjToVisCnvClp" Default="Collapsed"/>
            <UI:BoolToVisibility x:Key="BoolTovisInv" Inverted="True"/>
            <UI:BoolToVisibility x:Key="BoolTovis"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
